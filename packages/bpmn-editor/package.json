    "@kie-tools/pmml-editor-marshaller": "workspace:*",
    "@kie-tools/xml-parser-ts": "workspace:*",
    "@kie-tools/xyflow-react-kie-diagram": "workspace:*",
    "@patternfly/react-core": "^5.4.1",
