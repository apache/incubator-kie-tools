{
  "name": "vscode-extension-pmml-editor",
  "publisher": "redhat",
  "displayName": "PMML Editor",
  "description": "Edit PMML Scorecards",
  "version": "0.9.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kie-tooling-store.git"
  },
  "scripts": {
    "build-ext": "rm -rf dist && webpack --mode production --devtool none",
    "pack-ext": "vsce package --yarn -o ./dist/vscode_extension_pmml_editor_$npm_package_version.vsix",
    "pack": "yarn build-ext && yarn pack-ext",
    "compile": "yarn build-ext",
    "watch": "yarn build-ext"
  },
  "dependencies": {
    "@kogito-tooling/editor": "0.9.0",
    "@kogito-tooling/i18n": "0.9.0",
    "@kogito-tooling/patternfly-base": "0.9.0",
    "@kogito-tooling/pmml-editor": "0.9.0",
    "@kogito-tooling/vscode-extension": "0.9.0",
    "@kogito-tooling/workspace": "0.9.0"
  },
  "icon": "icon.png",
  "main": "./dist/extension/extension.js",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCustomEditor:kieKogitoWebviewEditorsPmml"
  ],
  "contributes": {
    "customEditors": [
      {
        "viewType": "kieKogitoWebviewEditorsPmml",
        "displayName": "PMML Editor",
        "selector": [
          {
            "filenamePattern": "*.{pmml}"
          }
        ]
      }
    ],
    "languages": [
      {
        "id": "pmml",
        "extensions": [
          ".pmml"
        ],
        "aliases": [
          "pmml",
          "Predictive Model Markup Language"
        ]
      }
    ]
  }
}
