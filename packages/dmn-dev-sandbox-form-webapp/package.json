{
  "private": true,
  "name": "@kie-tools/dmn-dev-sandbox-form-webapp",
  "version": "0.0.0",
  "description": "",
  "license": "Apache-2.0",
  "homepage": "https://github.com/kiegroup/kie-tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kie-tools.git"
  },
  "bugs": {
    "url": "https://github.com/kiegroup/kie-tools/issues"
  },
  "types": "./dist/index.d.ts",
  "main": "dist/index.js",
  "scripts": {
    "build:dev": "rimraf dist && webpack --env dev",
    "build:prod": "pnpm lint && pnpm test && rimraf dist && webpack",
    "lint": "run-script-if --bool \"$(build-env linters.run)\" --then \"pnpm eslint ./src --ext .ts,.tsx\"",
    "start": "webpack serve --host 0.0.0.0 --env dev",
    "test": "run-script-if --ignore-errors \"$(build-env tests.ignoreFailures)\" --bool \"$(build-env tests.run)\" --then \"jest --silent --verbose\""
  },
  "dependencies": {
    "@kie-tools-core/i18n": "workspace:*",
    "@kie-tools-core/patternfly-base": "workspace:*",
    "@kie-tools/form-dmn": "workspace:*",
    "@kie-tools/i18n-common-dictionary": "workspace:*",
    "@patternfly/react-core": "^4.157.3",
    "@patternfly/react-icons": "^4.11.17",
    "@types/react-router": "^5.1.14",
    "@types/react-router-dom": "^5.1.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.2.1"
  },
  "devDependencies": {
    "@kie-tools-build/root": "workspace:*",
    "@kie-tools-core/webpack-base": "workspace:*",
    "jest-junit": "^14.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/react"
    ]
  }
}