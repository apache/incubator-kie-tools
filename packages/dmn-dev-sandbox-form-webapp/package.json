{
  "name": "@kie-tools/dmn-dev-sandbox-form-webapp",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kie-tools.git"
  },
  "dependencies": {
    "@kie-tools-core/patternfly-base": "workspace:*",
    "@kie-tools-core/i18n": "workspace:*",
    "@kie-tools/form-dmn": "workspace:*",
    "@kie-tools/i18n-common-dictionary": "workspace:*",
    "@types/react-router": "^5.1.14",
    "@types/react-router-dom": "^5.1.7",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@patternfly/react-core": "^4.157.3",
    "@patternfly/react-icons": "^4.11.17"
  },
  "devDependencies": {
    "@kie-tools/build-env": "workspace:*",
    "@kie-tools-core/run-script-if": "workspace:*",
    "@kie-tools-core/webpack-base": "workspace:*"
  },
  "scripts": {
    "lint": "pnpm run-script-if --bool \"$(build-env global.build.lint)\" --then \"pnpm eslint ./src --ext .ts,.tsx\"",
    "test": "pnpm run-script-if --bool \"$(build-env global.build.test)\" --then \"jest --silent --verbose\"",
    "build:dev": "rimraf dist && webpack --env dev",
    "build:prod": "pnpm lint && pnpm test && rimraf dist && webpack",
    "start": "webpack serve --host 0.0.0.0 --env dev"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/react"
    ]
  }
}
