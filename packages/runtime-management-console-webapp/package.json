{
  "name": "@kie-tools/runtime-management-console-webapp",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "author": "",
  "keywords": [],
  "homepage": "https://github.com/apache/incubator-kie-tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/apache/incubator-kie-tools.git"
  },
  "bugs": {
    "url": "https://github.com/apache/incubator-kie-tools/issues"
  },
  "main": "index.js",
  "scripts": {
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "build:prod": "pnpm run lint && webpack --config webpack.prod.js && locktt",
    "bundle-profile:analyze": "pnpm build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "codegen": "graphql-codegen",
    "precommit": "lint-staged",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "concurrently 'pnpm  start' 'pnpm run dev:server'",
    "dev-remote-dataindex": "pnpm start --env KOGITO_DATAINDEX_HTTP_URL='http://localhost:8180/graphql'",
    "dev:restServer": "nodemon ./server/restServer.js",
    "dev:server": "nodemon ./server/app.js",
    "format": "prettier --config ../../.prettierrc --check --write './{src,cypress}/**/*.{tsx,ts,js}'",
    "lint": "kie-tools--eslint './{src,cypress}/**/*.ts{,x}'",
    "start": "webpack-dev-server --color --config webpack.dev.js",
    "start:it": "concurrently 'webpack --config webpack.it.js && pnpm serve -p 9000 -c ../serve.json ./dist-it/' 'pnpm run dev:server'",
    "test": "jest --runInBand --ci --reporters=default --reporters=jest-junit",
    "test:coverage": "rimraf coverage && pnpm run test --coverage",
    "test:it": "start-server-and-test 'pnpm run start:it' 9000 'pnpm run cypress:run' && echo",
    "test:report": "pnpm run test --ci --reporters=jest-junit",
    "update-snapshot": "jest --updateSnapshot"
  },
  "dependencies": {
    "@kie-tools/runtime-tools-components": "workspace:*",
    "@kie-tools/runtime-tools-process-enveloped-components": "workspace:*",
    "@kie-tools/runtime-tools-process-gateway-api": "workspace:*",
    "@kie-tools/runtime-tools-shared-enveloped-components": "workspace:*",
    "@kie-tools/runtime-tools-shared-webapp-components": "workspace:*",
    "@patternfly/patternfly": "^4.224.2",
    "@patternfly/react-core": "^4.276.8",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "1.1.13",
    "apollo-link-http": "1.5.17",
    "axios": "1.6.4",
    "history": "^4.10.1",
    "react": "^17.0.2",
    "react-apollo": "^3.1.5",
    "react-apollo-hooks": "^0.5.0",
    "react-dom": "^17.0.2",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@graphql-codegen/add": "^3.2.3",
    "@graphql-codegen/cli": "^2.16.5",
    "@graphql-codegen/introspection": "^2.2.3",
    "@graphql-codegen/typescript": "^2.8.8",
    "@graphql-codegen/typescript-operations": "^2.5.13",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@kie-tools/eslint": "workspace:*",
    "@kie-tools/runtime-cypress-ouia": "workspace:*",
    "@kie-tools/tsconfig": "workspace:*",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/enzyme": "^3.10.13",
    "@types/jest": "^26.0.24",
    "@types/react": "17.0.5",
    "@types/react-dom": "^17.0.20",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^4.41.33",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "apollo-server-express": "^2.26.1",
    "babel-jest": "^25.5.1",
    "concurrently": "^5.3.0",
    "core-js": "3.6.5",
    "css-loader": "^5.2.7",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cypress": "^12.17.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint-plugin-cypress": "^2.13.3",
    "express": "^4.18.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "https-browserify": "^1.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^14.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^2.0.22",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.6.0",
    "serve": "^12.0.1",
    "start-server-and-test": "^1.15.4",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.4.4",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.9.5",
    "url-loader": "^4.1.1",
    "uuid": "^3.4.0",
    "waait": "^1.0.5",
    "webpack": "^5.88.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "./src/static",
      "./src/apis/index.ts",
      "./src/pages/index.ts"
    ],
    "coverageReporters": [
      [
        "lcov",
        {
          "projectRoot": "../../"
        }
      ]
    ],
    "globals": {
      "ts-jest": {
        "isolatedModules": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(scss|sass|css)$": "identity-obj-proxy"
    },
    "preset": "ts-jest/presets/js-with-ts",
    "setupFiles": [
      "./config/Jest-config/test-shim.js",
      "./config/Jest-config/test-setup.js",
      "core-js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "**/tests/*.(ts|tsx)"
    ],
    "testPathIgnorePatterns": [
      "./src/index.tsx"
    ],
    "transform": {
      "\\.(jpg|jpeg|png|svg)$": "./config/Jest-config/fileMocks.js",
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.jsx?$": "./config/Jest-config/babel-jest-wrapper.js"
    },
    "transformIgnorePatterns": []
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "pnpm run format",
      "pnpm run lint"
    ]
  }
}