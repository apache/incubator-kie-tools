{
  "name": "@kie-tools/unitables",
  "version": "0.0.0",
  "description": "",
  "license": "Apache-2.0",
  "keywords": [],
  "homepage": "https://github.com/kiegroup/kie-tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kie-tools.git"
  },
  "bugs": {
    "url": "https://github.com/kiegroup/kie-tools/issues"
  },
  "module": "./dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:dev": "rimraf dist && pnpm copy:css && tsc -p tsconfig.dev.json",
    "build:prod": "rimraf dist && pnpm lint && pnpm copy:css && tsc -p tsconfig.dev.json && pnpm test",
    "build:watch": "rimraf dist && pnpm copy:css && tsc -p tsconfig.dev.json --watch",
    "copy:css": "copyfiles -u 1 \"src/**/*.{sass,scss,css}\" dist/",
    "lint": "run-script-if --bool \"$(build-env linters.run)\" --then \"pnpm eslint ./src --ext .ts,.tsx\"",
    "test": "run-script-if --ignore-errors \"$(build-env tests.ignoreFailures)\" --bool \"$(build-env tests.run)\" --then \"jest --silent --verbose --passWithNoTests\""
  },
  "dependencies": {
    "@kie-tools-core/i18n": "workspace:*",
    "@kie-tools-core/notifications": "workspace:*",
    "@kie-tools/boxed-expression-component": "workspace:*",
    "@kie-tools/form": "workspace:*",
    "@kie-tools/i18n-common-dictionary": "workspace:*",
    "@patternfly/react-core": "^4.157.3",
    "@patternfly/react-icons": "^4.11.17",
    "@types/lodash": "^4.14.168",
    "@types/react-table": "^7.0.25",
    "ajv": "^6.12.6",
    "ajv-errors": "^1.0.1",
    "deep-object-diff": "^1.1.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-id-generator": "^3.0.1",
    "react-table": "^7.6.2",
    "uniforms": "^3.5.5",
    "uniforms-bridge-json-schema": "^3.5.5",
    "uniforms-patternfly": "^4.7.5"
  },
  "devDependencies": {
    "@kie-tools/root-env": "workspace:*",
    "@testing-library/jest-dom": "^5.16.1",
    "@types/jest": "^26.0.23",
    "@types/jest-when": "^2.7.4",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/testing-library__jest-dom": "^5.9.1",
    "copyfiles": "^2.4.1",
    "jest": "^26.6.3",
    "jest-junit": "^14.0.0",
    "jest-when": "^3.5.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.4.2"
  }
}