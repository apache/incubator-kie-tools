<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:bootstrap="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:gwt="urn:import:com.google.gwt.user.client.ui" >

  <ui:with field='i18n' type='org.kie.workbench.common.widgets.client.resources.i18n.CommonConstants'/>
  <ui:with field='resources' type='org.kie.workbench.common.widgets.client.resources.CommonsResources'/>


    <gwt:HTMLPanel>
      <bootstrap:FormLabel ui:field="parameterName" styleName="form-control-label"/>
      <bootstrap:TextBox ui:field="parameterEditor" styleName="form-control"/>
      <bootstrap:ListBox ui:field="lstAvailableBindings" visible="false" enabled="false" styleName="form-control"/>
    </gwt:HTMLPanel>

</ui:UiBinder>