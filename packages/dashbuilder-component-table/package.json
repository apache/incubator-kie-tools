{
  "name": "@kie-tools/dashbuilder-component-table",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kie-tools.git"
  },
  "dependencies": {
    "@kie-tools/dashbuilder-component-api": "0.0.0",
    "@kie-tools-core/patternfly-base": "0.0.0",
    "@patternfly/react-core": "^4.157.3",
    "@patternfly/react-table": "^4.27.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@kie-tools-core/run-script-if": "0.0.0",
    "@kie-tools/build-env": "0.0.0",
    "@kie-tools/dashbuilder-component-dev": "0.0.0",
    "@kie-tools-core/webpack-base": "0.0.0"
  },
  "scripts": {
    "lint": "pnpm run-script-if --bool \"$(build-env global.build.lint)\" --then \"pnpm eslint ./src --ext .ts,.tsx\"",
    "test": "pnpm run-script-if --bool \"$(build-env global.build.test)\" --then \"jest --silent --verbose --passWithNoTests\"",
    "copy:css": "copyfiles -u 1 \"src/**/*.{sass,scss,css}\" dist/",
    "build:dev": "rimraf dist && pnpm copy:css && webpack --env dev",
    "build:prod": "pnpm lint && pnpm test && rimraf dist && pnpm copy:css && webpack",
    "start": "webpack serve --host 0.0.0.0 --env WEBPACK_SERVE"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/react"
    ]
  }
}
