{
  "name": "@kie-tools/serverless-workflow-language-service",
  "version": "0.0.0",
  "description": "",
  "license": "Apache-2.0",
  "keywords": [],
  "homepage": "https://github.com/kiegroup/kie-tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kie-tools.git"
  },
  "bugs": {
    "url": "https://github.com/kiegroup/kie-tools/issues"
  },
  "types": "./dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build:dev": "rimraf dist && tsc -p tsconfig.dev.json",
    "build:prod": "pnpm lint && rimraf dist && tsc -p tsconfig.prod.json && pnpm test",
    "lint": "run-script-if --bool \"$(build-env linters.run)\" --then \"pnpm eslint ./src --ext .ts,.tsx\"",
    "test": "run-script-if --ignore-errors \"$(build-env tests.ignoreFailures)\" --bool \"$(build-env tests.run)\" --then \"jest --silent --verbose --passWithNoTests\""
  },
  "dependencies": {
    "@kie-tools/serverless-workflow-service-catalog": "workspace:*",
    "@severlessworkflow/sdk-typescript": "^3.0.3",
    "json-to-ast": "^2.1.0",
    "jsonata": "^1.8.3",
    "jsonc-parser": "^3.0.0",
    "vscode-json-languageservice": "^4.2.1",
    "vscode-languageserver-textdocument": "^1.0.4",
    "vscode-languageserver-types": "^3.16.0",
    "yaml-language-server-parser": "^0.1.3"
  },
  "devDependencies": {
    "@kie-tools-build/root": "workspace:*",
    "@types/json-to-ast": "^2.1.2",
    "jest-junit": "^14.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/react"
    ]
  }
}