    "@types/json-to-ast": "^2.1.2",
    "jest": "^26.6.3",
    "jest-junit": "^14.0.0",
    "jest-when": "^3.5.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.4.2"
  }
