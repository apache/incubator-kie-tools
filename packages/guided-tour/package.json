{
  "name": "@kogito-tooling/guided-tour",
  "version": "0.9.1",
  "description": "Guided Tour component",
  "license": "Apache-2.0",
  "source": "src/index.tsx",
  "scripts": {
    "lint": "echo 'Linting..'",
    "test": "jest --silent --verbose",
    "test:clearCache": "jest --clearCache",
    "build:fast": "rm -rf dist && webpack",
