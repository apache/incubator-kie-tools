{
  "name": "@kogito-tooling/guided-tour",
  "version": "0.8.3",
  "description": "Guided Tour component",
  "license": "Apache-2.0",
  "source": "src/index.tsx",
  "scripts": {
    "lint": "tslint -c ../../tslint.json 'src/**/*.{ts,tsx,js,jsx}'",
    "test": "jest --silent --verbose",
    "test:clearCache": "jest --clearCache",
    "build:fast": "rm -rf dist && webpack",
    "build": "yarn run lint && yarn test && yarn run build:fast",
    "build:prod": "yarn run build --mode production --devtool none",
    "start": "rm -rf dist && webpack --watch"
  },
  "peerDependencies": {
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "dependencies": {
    "@kogito-tooling/envelope-bus": "0.8.3",
    "@kogito-tooling/i18n": "0.8.3",
    "@kogito-tooling/i18n-common-dictionary": "0.8.3"
  },
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "files": [
    "dist"
  ]
}
