    "build:prod": "yarn run build #mode production",
    "start": "rm -rf dist && webpack --watch"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@kogito-tooling/envelope-bus": "0.9.1",
    "@kogito-tooling/i18n": "0.9.1",
    "@kogito-tooling/i18n-common-dictionary": "0.9.1"
  },
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "files": [
    "dist"
  ]
}
