      <bpmn2:outgoing>_903EE03E-3B2F-4C4E-8B2C-7264B8888B3E</bpmn2:outgoing>
      <bpmn2:script>logger.info("Performing calculation: " + x + " " + operator + " " + y + " = result");
char op = operator.charAt(0);
switch(op) {
    case '+':
        result = x + y;
        break;
    case '-':
        result = x - y;
        break;
    case '*':
        result = x * y;
        break;
    case '/':
        result = x / y;
        break;
    default:
        System.out.println("ERROR: Invalid operator " + operator);
        throw new IllegalArgumentException("Invalid operator " + operator);
}
kcontext.setVariable("result", result);
</bpmn2:script>
    </bpmn2:scriptTask>
