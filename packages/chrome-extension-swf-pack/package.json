{
  "name": "chrome-extension-swf-pack",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kie-tools.git"
  },
  "dependencies": {
    "@kie-tools-core/chrome-extension-swf": "0.0.0",
    "@kie-tools-core/patternfly-base": "0.0.0",
    "@kie-tools/serverless-workflow-editor": "0.0.0"
  },
  "devDependencies": {
    "@kie-tools/build-env": "0.0.0",
    "@kie-tools-core/webpack-base": "0.0.0"
  },
  "scripts": {
    "lint": "yarn run run-script-if --bool \"$(build-env global.build.lint)\" --then \"yarn eslint ./src --ext .ts,.tsx\"",
    "test": "yarn run run-script-if --bool \"$(build-env global.build.test)\" --then \"jest --silent --verbose --passWithNoTests\"",
    "build:dev": "rimraf dist && webpack --env dev",
    "build:prod": "yarn lint && yarn test && rimraf dist && webpack",
    "watch:prod": "webpack watch",
    "start": "webpack serve --env dev"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/react"
    ]
  }
}
