{
  "name": "@kie-tools-core/chrome-extension",
  "version": "0.0.0",
  "description": "",
  "license": "Apache-2.0",
  "keywords": [],
  "homepage": "https://github.com/kiegroup/kie-tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kie-tools.git"
  },
  "bugs": {
    "url": "https://github.com/kiegroup/kie-tools/issues"
  },
  "types": "./dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist",
    "resources",
    "src"
  ],
  "scripts": {
    "build:dev": "rimraf dist && tsc -p tsconfig.dev.json",
    "build:prod": "pnpm lint && rimraf dist && tsc -p tsconfig.prod.json && pnpm test",
    "lint": "run-script-if --bool \"$(build-env linters.run)\" --then \"pnpm eslint ./src --ext .ts,.tsx\"",
    "test": "run-script-if --ignore-errors \"$(build-env tests.ignoreFailures)\" --bool \"$(build-env tests.run)\" --then \"jest --silent --verbose --passWithNoTests\""
  },
  "dependencies": {
    "@kie-tools-core/editor": "workspace:*",
    "@kie-tools-core/i18n": "workspace:*",
    "@kie-tools-core/patternfly-base": "workspace:*",
    "@kie-tools-core/workspace": "workspace:*",
    "@octokit/rest": "^18.5.3",
    "minimatch": "^3.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@kie-tools/root-env": "workspace:*",
    "@types/chrome": "^0.0.193",
    "@types/minimatch": "^3.0.4",
    "jest-junit": "^14.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/react"
    ]
  }
}