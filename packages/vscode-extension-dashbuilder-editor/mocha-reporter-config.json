{
  "reporterEnabled": "mocha-jenkins-reporter, mocha-junit-reporter",
  "mochaJenkinsReporterReporterOptions": {
    "junit_report_name": "@kie-tools/vscode-extension-dashbuilder-editor",
    "junit_report_path": "./dist-tests-e2e/junit-vscode-sl-tooling-e2e-tests.xml",
    "junit_report_stack": 0
  },
  "mochaJunitReporterReporterOptions": {
    "testsuitesTitle": "Dashbuilder Editor :: VS Code Extension",
    "mochaFile": "./dist-tests-e2e/junit-report-[hash].xml",
    "testCaseSwitchClassnameAndName": true,
    "suiteTitleSeparatedBy": ".",
    "useFullSuiteTitle": true
  }
}
