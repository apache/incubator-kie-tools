  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@kie-tools/eslint": "workspace:*",
    "@kie-tools/root-env": "workspace:*",
    "@kie-tools/tsconfig": "workspace:*",
    "@types/jest": "^26.0.23",
    "esbuild": "^0.15.13",
    "jest": "^26.6.3",
    "jest-junit": "^14.0.0",
    "jest-when": "^3.5.0",
