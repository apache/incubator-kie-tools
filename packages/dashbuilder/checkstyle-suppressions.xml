<?xml version="1.0"?>
 
<!DOCTYPE suppressions PUBLIC
     "-//Checkstyle//DTD SuppressionFilter Configuration 1.0//EN"
     "https://checkstyle.org/dtds/suppressions_1_0.dtd">
 
<suppressions>
  <suppress checks="FileTabCharacter"
            files=".*\.[java,properties]"/>
  <suppress checks="RegexpHeader"
            files=".*\.java"/>
  <suppress checks="NeedBraces"
            files=".*\.java"/>
  <suppress checks="EqualsHashCode"
            files=".*\.java"/>
</suppressions>