<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:b='urn:import:org.gwtbootstrap3.client.ui'>

  <ui:with field="i18n" type="org.uberfire.ext.widgets.core.client.resources.i18n.CoreConstants"/>

  <b:ModalFooter>
    <b:Button ui:field="btnPrevious" icon="ANGLE_LEFT" iconPosition="LEFT" text="{i18n.Previous}" />
    <b:Button ui:field="btnNext" icon="ANGLE_RIGHT" iconPosition="RIGHT" text="{i18n.Next}" />
    <b:Button ui:field="btnCancel" text="{i18n.cancel}" />
    <b:Button ui:field="btnFinish" icon="CHECK" type="PRIMARY" text="{i18n.Finish}" />
  </b:ModalFooter>

</ui:UiBinder>