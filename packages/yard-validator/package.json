  "dependencies": {
    "@kie-tools-core/notifications": "workspace:*"
  },
  "devDependencies": {
    "@kie-tools/eslint": "workspace:*",
    "@kie-tools/root-env": "workspace:*",
    "@kie-tools/tsconfig": "workspace:*",
    "@kie-tools/yard-validator-worker": "workspace:*",
    "copyfiles": "^2.4.1",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "typescript": "^5.9.3"
  }
