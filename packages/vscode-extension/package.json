{
  "name": "@kie-tools-core/vscode-extension",
  "displayName": "KIE :: Kogito Editors",
  "description": "",
  "version": "0.21.1",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kie-tools.git"
  },
  "publisher": "kie-group",
  "scripts": {
    "compile": "webpack",
    "watch": "webpack",
    "lint": "pnpm run-script-if --bool \"$(build-env global.build.lint)\" --then \"pnpm eslint ./src --ext .ts,.tsx\"",
    "test": "pnpm run-script-if --bool \"$(build-env global.build.test)\" --then \"jest --silent --verbose --passWithNoTests\"",
    "build:dev": "rimraf dist && tsc -p tsconfig.dev.json",
    "build:prod": "rimraf dist && pnpm lint && tsc -p tsconfig.prod.json && pnpm test"
  },
  "dependencies": {
    "@kie-tools-core/backend": "0.21.1",
    "@kie-tools-core/editor": "0.21.1",
    "@kie-tools-core/envelope-bus": "0.21.1",
    "@kie-tools-core/guided-tour": "0.21.1",
    "@kie-tools-core/i18n": "0.21.1",
    "@kie-tools-core/keyboard-shortcuts": "0.21.1",
    "@kie-tools-core/notifications": "0.21.1",
    "@kie-tools-core/vscode-java-code-completion": "0.21.1",
    "@kie-tools-core/workspace": "0.21.1",
    "minimatch": "^3.0.4"
  },
  "devDependencies": {
    "@kie-tools-core/run-script-if": "0.21.1",
    "@kie-tools/build-env": "0.21.1",
    "@types/minimatch": "^3.0.4"
  }
}
