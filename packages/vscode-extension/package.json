{
  "name": "@kogito-tooling/vscode-extension",
  "displayName": "KIE :: Kogito Editors",
  "description": "",
  "version": "0.11.0",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kogito-tooling.git"
  },
  "publisher": "kie-group",
  "scripts": {
    "build:dev": "rimraf dist && webpack --env dev",
    "compile": "webpack",
    "watch": "webpack",
    "lint": "run-script-if --env KOGITO_TOOLING_BUILD_lint -c 'yarn eslint ./src --ext .ts,.tsx'",
    "test": "run-script-if --env KOGITO_TOOLING_BUILD_test -c 'jest --silent --verbose'",
    "test:clearCache": "jest --clearCache",
    "build:prod": "rimraf dist && yarn lint && yarn test && webpack"
  },
  "dependencies": {
    "@kogito-tooling/backend": "0.11.0",
    "@kogito-tooling/editor": "0.11.0",
    "@kogito-tooling/i18n": "0.11.0",
    "@kogito-tooling/keyboard-shortcuts": "0.11.0",
    "@kogito-tooling/workspace": "0.11.0"
  }
}
