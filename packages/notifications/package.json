{
  "name": "@kogito-tooling/notifications",
  "version": "0.10.0",
  "description": "",
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kogito-tooling.git"
  },
  "scripts": {
    "lint": "run-script-if --env KOGITO_TOOLING_BUILD_lint -c 'yarn eslint ./src --ext .ts,.tsx'",
    "test": "run-script-if --env KOGITO_TOOLING_BUILD_test -c 'jest --silent --verbose --passWithNoTests'",
    "test:clearCache": "jest --clearCache",
    "build:dev": "rimraf dist && webpack --env dev",
    "build:prod": "yarn run lint && yarn test && rimraf dist && webpack"
  },
  "dependencies": {
    "@kogito-tooling/i18n": "0.10.0",
    "@kogito-tooling/i18n-common-dictionary": "0.10.0",
    "@kogito-tooling/workspace": "0.10.0"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ]
  },
  "jest-junit": {
    "outputDirectory": "./target"
  }
}
