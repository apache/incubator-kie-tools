{
  "name": "@kogito-tooling/kie-bc-editors",
  "version": "0.0.1",
  "description": "",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kogito-tooling.git"
  },
  "dependencies": {
    "@kogito-tooling/build-env": "0.0.1",
    "@kogito-tooling/pmml-editor-marshaller": "0.0.1"
  },
  "scripts": {
    "lint": "yarn run run-script-if --bool \"$(build-env global.build.lint)\" --then \"yarn eslint ./src --ext .ts,.tsx\"",
    "test": "yarn run run-script-if --bool \"$(build-env global.build.test)\" --then \"jest --silent --verbose --passWithNoTests\"",
    "test:clearCache": "jest --clearCache",
    "build:dev": "rimraf dist && webpack --env dev",
    "build:prod": "yarn lint && yarn test && rimraf dist && webpack"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ]
  }
}
