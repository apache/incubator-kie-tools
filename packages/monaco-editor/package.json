{
  "name": "@kie-tools-core/monaco-editor",
  "version": "0.0.0",
  "description": "Single-file build of monaco-editor",
  "license": "MIT",
  "keywords": [
    "monaco"
  ],
  "types": "dist/standalone/monaco.min.d.ts",
  "main": "dist/standalone/monaco.min.js",
  "files": [
    "dist/standalone"
  ],
  "scripts": {
    "build:dev": "rimraf dist && webpack --mode development",
    "build:prod": "rimraf dist && webpack --mode production",
    "lint": "run-script-if --bool \"$(build-env linters.run)\" --then \"kie-tools--eslint ./src\"",
    "test": "run-script-if --ignore-errors \"$(build-env tests.ignoreFailures)\" --bool \"$(build-env tests.run)\" --then \"jest --silent --verbose --passWithNoTests\""
  },
  "dependencies": {
    "monaco-editor": "^0.39.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@kie-tools/eslint": "workspace:*",
    "@kie-tools/root-env": "workspace:*",
    "@kie-tools/tsconfig": "workspace:*",
    "@types/jest": "^26.0.23",
    "@types/jest-when": "^2.7.4",
    "css-loader": "^5.2.6",
    "jest": "^26.6.3",
    "jest-when": "^3.5.0",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.4.2",
    "typescript": "^4.6.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  }
}