{
  "name": "@kie-tools-core/monaco-editor",
  "version": "0.0.0",
  "description": "Single-file build of monaco-editor",
  "license": "MIT",
  "keywords": [
    "monaco"
  ],
  "types": "dist/standalone/monaco.min.d.ts",
  "main": "dist/standalone/monaco.min.js",
  "files": [
    "dist/standalone"
  ],
  "scripts": {
    "build:dev": "rimraf dist && webpack --mode development",
    "build:prod": "rimraf dist && webpack --mode production",
    "lint": "run-script-if --bool \"$(build-env linters.run)\" --then \"pnpm eslint ./src --ext .ts,.tsx\"",
    "test": "run-script-if --bool \"$(build-env tests.run)\" --then \"jest --silent --verbose --passWithNoTests\""
  },
  "dependencies": {
    "monaco-editor": "^0.33.0"
  },
  "devDependencies": {
    "@kie-tools-core/run-script-if": "workspace:*",
    "@kie-tools/build-env": "workspace:*"
  }
}