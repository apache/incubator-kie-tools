{
  "private": true,
  "name": "@kie-tools/cors-proxy-image",
  "version": "0.0.0",
  "description": "",
  "license": "Apache-2.0",
  "homepage": "https://github.com/kiegroup/kie-tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kie-tools.git"
  },
  "bugs": {
    "url": "https://github.com/kiegroup/kie-tools/issues"
  },
  "scripts": {
    "build:dev": "echo \"Nothing to do\"",
    "build:prod": "pnpm cleanup && pnpm copy:cors-proxy-package && run-script-os",
    "build:prod:linux:darwin": "run-script-if --bool \"$(build-env containerImages.build)\" --then \"pnpm podman:build\"",
    "build:prod:win32": "echo \"Build not supported on Windows\"",
    "cleanup": "rimraf dist-dev && mkdir dist-dev",
    "copy:cors-proxy-package": "run-script-os",
    "copy:cors-proxy-package:linux:darwin": "cp -R ./node_modules/@kie-tools/cors-proxy/ dist-dev/cors-proxy/",
    "copy:git-cors-proxy-package:win32": "pnpm powershell \"Copy-Item -R ../node_modules/@kie-tools/cors-proxy/ dist-dev/cors-proxy/\"",
    "docker:build": "kie-tools--image-builder -r \"$(build-env corsProxy.image.registry)\" -a \"$(build-env corsProxy.image.account)\" -n \"$(build-env corsProxy.image.name)\" -t \"$(build-env corsProxy.image.buildTags)\"",
    "podman:build": "kie-tools--image-builder -r \"$(build-env corsProxy.image.registry)\" -a \"$(build-env corsProxy.image.account)\" -n \"$(build-env corsProxy.image.name)\" -t \"$(build-env corsProxy.image.buildTags)\" -e podman"
  },
  "devDependencies": {
    "@kie-tools/cors-proxy": "workspace:*",
    "@kie-tools/image-builder": "workspace:*",
    "@kie-tools/root-env": "workspace:*",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6"
  }
}