    "mini-css-extract-plugin": "^2.7.6",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "nodemon": "^2.0.22",
    "openapi-types": "^9.3.1",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.3.0",
    "stream-http": "^3.2.0",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^8.0.0",
    "swagger-ui-express": "4.3.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.4.2",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.2",
    "url-loader": "^4.1.1",
    "uuid": "^8.3.2",
    "waait": "^1.0.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "pnpm run format",
      "pnpm run lint"
    ]
  }
}