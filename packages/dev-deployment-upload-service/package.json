{
  "private": true,
  "name": "@kie-tools/dev-deployment-upload-service",
  "version": "0.0.0",
  "description": "",
  "license": "Apache-2.0",
  "homepage": "https://github.com/apache/incubator-kie-tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/apache/incubator-kie-tools.git"
  },
  "bugs": {
    "url": "https://github.com/apache/incubator-kie-tools/issues"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-script-os",
    "build-all": "run-script-os",
    "build-all:linux:darwin": "cross-env DDUS_VERSION=$(build-env root.version) make build-all",
    "build-all:win32": "pnpm powershell \"cross-env DDUS_VERSION=$(build-env root.version) make build-all\"",
    "build-install-script:dev": "node scripts/generateInstallScript.js --dev",
    "build-install-script:prod": "node scripts/generateInstallScript.js",
    "build:darwin:amd64": "cross-env DDUS_VERSION=$(build-env root.version) make darwin-amd64",
    "build:darwin:arm64": "cross-env DDUS_VERSION=$(build-env root.version) make darwin-arm64",
    "build:dev": "rimraf dist && pnpm build-install-script:dev && pnpm build && make checksum",
    "build:linux": "cross-env DDUS_VERSION=$(build-env root.version) make linux-amd64",
    "build:prod": "rimraf dist && pnpm build-install-script:prod && pnpm build-all",
    "build:win32": "pnpm powershell \"cross-env DDUS_VERSION=$(build-env root.version) make win32-amd64\"",
    "install": "go mod tidy",
    "powershell": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command",
    "serve": "run-script-os",
    "serve:linux:darwin": "serve -c dev/serve.json -p $(build-env devDeploymentUploadService.dev.port)",
    "serve:win32": "pnpm powershell \"serve -c dev/serve.json -p $(build-env devDeploymentUploadService.dev.port)\"",
    "start": "make start",
    "start:dev": "pnpm build:prod && docker run -it --add-host=host.docker.internal:host-gateway -p 8091:8091 $(docker build --no-cache --build-arg INSTALL_SCRIPT_PORT=$(build-env devDeploymentUploadService.dev.port) -q . -f ./dev/Containerfile)"
  },
  "devDependencies": {
    "@kie-tools/root-env": "workspace:*",
    "cross-env": "^7.0.3",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "serve": "^14.2.1"
  },
  "kieTools": {
    "requiredPreinstalledCliCommands": [
      "go",
      "make"
    ]
  }
}