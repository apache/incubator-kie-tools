      <artifactId>kie-wb-common-ui</artifactId>
    </dependency>

    <dependency>
      <groupId>com.google.elemental2</groupId>
      <artifactId>elemental2-dom</artifactId>
    </dependency>

    <dependency>
      <groupId>org.kie.kogito.stunner.serverless.editor</groupId>
