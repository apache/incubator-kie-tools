{
  "reporterEnabled": "mocha-jenkins-reporter, mocha-junit-reporter",
  "mochaJenkinsReporterReporterOptions": {
    "junit_report_name": "@kie-tools/serverless-workflow-vscode-extension",
    "junit_report_path": "./dist-e2e-tests/junit-vscode-sl-tooling-e2e-tests.xml",
    "junit_report_stack": 0
  },
  "mochaJunitReporterReporterOptions": {
    "testsuitesTitle": "Serverless Workflow :: VS Code Extension",
    "mochaFile": "./dist-e2e-tests/junit-report-[hash].xml",
    "testCaseSwitchClassnameAndName": true,
    "suiteTitleSeparatedBy": ".",
    "useFullSuiteTitle": true
  }
}
