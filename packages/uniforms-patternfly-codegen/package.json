{
  "name": "@kie-tools/uniforms-patternfly-codegen",
  "version": "0.0.0",
  "description": "",
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kie-tools.git"
  },
  "scripts": {
    "lint": "pnpm run-script-if --bool \"$(build-env global.build.lint)\" --then \"pnpm eslint ./src --ext .ts,.tsx\"",
    "test": "pnpm run-script-if --bool \"$(build-env global.build.test)\" --then \"jest --silent --verbose --passWithNoTests\"",
    "build:dev": "rimraf dist && webpack --env dev",
    "build:prod": "pnpm lint && pnpm test && rimraf dist && webpack"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "prettier": "^2.2.1",
    "uniforms": "^3.5.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "uniforms-bridge-simple-schema-2": "^3.5.5"
  },
  "devDependencies": {
    "@kie-tools/build-env": "0.0.0",
    "@kie-tools-core/run-script-if": "0.0.0",
    "@kie-tools-core/webpack-base": "0.0.0",
    "@types/lodash": "^4.14.168",
    "@types/simpl-schema": "^1.12.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/react"
    ]
  }
}
