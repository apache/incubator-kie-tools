{
  "private": true,
  "name": "vscode-extension-backend",
  "version": "0.0.0",
  "description": "",
  "license": "Apache-2.0",
  "publisher": "kie-group",
  "homepage": "https://github.com/kiegroup/kie-tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kie-tools.git"
  },
  "bugs": {
    "url": "https://github.com/kiegroup/kie-tools/issues"
  },
  "main": "dist/extension/extension.js",
  "scripts": {
    "build:dev": "rimraf dist && webpack --env dev",
    "build:prod": "run-script-os",
    "build:prod:linux:darwin": "rimraf dist && pnpm lint && webpack && pnpm test && pnpm test:it && pnpm package:prod",
    "build:prod:windows": "rimraf dist && pnpm lint && webpack && pnpm test && pnpm package:prod",
    "compile": "webpack",
    "lint": "run-script-if --bool \"$(build-env linters.run)\" --then \"kie-tools--eslint ./src\"",
    "package:prod": "vsce package --no-dependencies -o ./dist/vscode-extension-backend_$npm_package_version.vsix",
    "test": "run-script-if --ignore-errors \"$(build-env tests.ignoreFailures)\" --bool \"$(build-env tests.run)\" --then \"jest --silent --verbose --passWithNoTests\"",
    "test:it": "echo 'No tests to run.'",
    "watch": "webpack"
  },
  "dependencies": {
    "@kie-tools-core/backend": "workspace:*",
    "@kie-tools-core/vscode-extension": "workspace:*",
    "@kie-tools/backend-extended-services": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@kie-tools-core/webpack-base": "workspace:*",
    "@kie-tools/eslint": "workspace:*",
    "@kie-tools/root-env": "workspace:*",
    "@kie-tools/tsconfig": "workspace:*",
    "@types/jest": "^26.0.23",
    "@types/jest-when": "^2.7.4",
    "@types/vscode": "^1.66.0",
    "copy-webpack-plugin": "^11.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^14.0.0",
    "jest-when": "^3.5.0",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "ts-jest": "^26.5.6",
    "vsce": "^2.6.5",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^5.8.0"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "displayName": "Backend services for BPMN, DMN and Test Scenario Editors",
  "categories": [
    "Other"
  ],
  "contributes": {
    "languages": [
      {
        "aliases": [
          "bpmn",
          "bpmn2",
          "Business Process Model and Notation"
        ],
        "extensions": [
          ".bpmn",
          ".bpmn2"
        ],
        "id": "bpmn"
      },
      {
        "aliases": [
          "dmn",
          "Decision Model and Notation"
        ],
        "extensions": [
          ".dmn"
        ],
        "id": "dmn"
      },
      {
        "aliases": [
          "scesim",
          "Test Scenario"
        ],
        "extensions": [
          ".scesim"
        ],
        "id": "scesim"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:bpmn",
    "onLanguage:dmn",
    "onLanguage:scesim"
  ],
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": false
    }
  }
}