  },
  "bugs": {
    "url": "https://github.com/kiegroup/kie-tools/issues"
  },
  "types": "./dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:dev": "rimraf dist && tsc -p tsconfig.json && pnpm copy:css",
    "build:prod": "pnpm lint && rimraf dist && tsc -p tsconfig.json && pnpm copy:css && pnpm test",
    "copy:css": "copyfiles -u 1 \"src/**/*.{sass,scss,css,svg,png}\" dist/",
    "lint": "run-script-if --bool \"$(build-env linters.run)\" --then \"kie-tools--eslint ./src\"",
    "test": "run-script-if --ignore-errors \"$(build-env tests.ignoreFailures)\" --bool \"$(build-env tests.run)\" --then \"jest --silent --verbose --passWithNoTests\""
  },
  "dependencies": {
    "@kie-tools-core/editor": "workspace:*",
    "@kie-tools-core/envelope": "workspace:*",
    "@kie-tools-core/envelope-bus": "workspace:*",
    "@kie-tools-core/i18n": "workspace:*",
    "@kie-tools-core/notifications": "workspace:*",
    "@kie-tools-core/patternfly-base": "workspace:*",
    "@kie-tools/i18n-common-dictionary": "workspace:*",
    "@kie-tools/runtime-tools-components": "workspace:*",
    "@kie-tools/runtime-tools-gateway-api": "workspace:*",
    "@kie-tools/serverless-workflow-combined-editor": "workspace:*",
    "@patternfly/react-core": "^4.276.6",
    "@patternfly/react-icons": "^4.93.6",
    "@patternfly/react-table": "^4.112.39",
    "copyfiles": "^2.4.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-json-view": "^1.21.3",
    "react-moment": "0.9.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@kie-tools-core/webpack-base": "workspace:*",
    "@kie-tools/eslint": "workspace:*",
    "@kie-tools/root-env": "workspace:*",
    "@kie-tools/tsconfig": "workspace:*",
    "@testing-library/jest-dom": "^6.1.4",
    "@types/jest": "^29.5.7",
    "@types/jest-when": "^3.5.4",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@types/uuid": "^8.3.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-when": "^3.6.0",
