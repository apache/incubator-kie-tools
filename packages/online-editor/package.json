  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kogito-tooling.git"
  },
  "dependencies": {
    "@kogito-tooling/core-api": "0.5.2",
    "@kogito-tooling/embedded-editor": "0.5.2",
    "@kogito-tooling/patternfly-base": "0.5.2",
    "@octokit/rest": "16.35.0",
