{
  "name": "@kogito-tooling/online-editor",
  "version": "0.10.0",
  "description": "",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kogito-tooling.git"
  },
  "dependencies": {
    "@kie-tooling-core/editor": "^0.0.1",
    "@kie-tooling-core/monaco-editor": "^0.0.1",
    "@kogito-tooling/external-assets-base": "0.10.0",
    "@kogito-tooling/i18n-common-dictionary": "0.10.0",
    "@kogito-tooling/kie-bc-editors": "0.10.0",
    "@kogito-tooling/pmml-editor": "0.10.0",
    "@octokit/rest": "^18.5.3",
    "@types/react-router": "^5.1.14",
    "@types/react-router-dom": "^5.1.7",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "scripts": {
    "lint": "run-script-if --env KOGITO_TOOLING_BUILD_lint --true-if-empty --then 'yarn eslint ./src --ext .ts,.tsx'",
    "test": "run-script-if --env KOGITO_TOOLING_BUILD_test --true-if-empty --then 'jest --silent --verbose'",
    "test:clearCache": "jest --clearCache",
    "build:dev": "rimraf dist && webpack --env dev",
    "build:prod": "yarn lint && yarn test && rimraf dist && webpack && yarn test:it",
    "start": "webpack serve --host 0.0.0.0",
    "cy:open": "yarn run cypress open --project it-tests",
    "cy:run": "yarn run cypress run -b chrome --project it-tests",
    "test:it": "run-script-if --env KOGITO_TOOLING_BUILD_testIT --then 'yarn run start-server-and-test start http://0.0.0.0:9001 cy:run'"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ]
  }
}
