{
  "name": "@kie-tools/form-dmn",
  "version": "0.0.0",
  "description": "",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kie-tools.git"
  },
  "scripts": {
    "lint": "pnpm run-script-if --bool \"$(build-env global.build.lint)\" --then \"pnpm eslint ./src --ext .ts,.tsx\"",
    "test": "pnpm run-script-if --bool \"$(build-env global.build.test)\" --then \"jest  --verbose --passWithNoTests\"",
    "copy:css": "copyfiles -u 1 \"src/**/*.{sass,scss,css}\" dist/",
    "build:dev": "rimraf dist && pnpm copy:css && tsc -p tsconfig.dev.json",
    "build:prod": "pnpm lint && rimraf dist && pnpm copy:css && tsc -p tsconfig.prod.json && pnpm test"
  },
  "dependencies": {
    "@kie-tools-core/i18n": "workspace:*",
    "@kie-tools-core/notifications": "workspace:*",
    "@kie-tools/form": "workspace:*",
    "@kie-tools/i18n-common-dictionary": "workspace:*",
    "ajv": "^6.12.6",
    "ajv-errors": "^1.0.1",
    "deep-object-diff": "^1.1.0",
    "lodash": "^4.17.21",
    "uniforms": "^3.5.5",
    "uniforms-bridge-json-schema": "^3.5.5",
    "uniforms-patternfly": "^4.7.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@patternfly/react-core": "^4.157.3",
    "@patternfly/react-icons": "^4.11.17"
  },
  "devDependencies": {
    "@kie-tools/build-env": "workspace:*",
    "@kie-tools-core/run-script-if": "workspace:*",
    "@kie-tools-core/webpack-base": "workspace:*",
    "@types/lodash": "^4.14.168"
  }
}
