{
  "compilerOptions": {
    "baseUrl": "./it-tests",
    "module": "commonjs",
    "outDir": "./dist",
    "jsx": "react-jsxdev",
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": false,
    "declarationMap": false,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "strictNullChecks": true,
    "experimentalDecorators": false,
