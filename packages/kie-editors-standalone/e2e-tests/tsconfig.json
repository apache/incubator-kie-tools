{
  "compilerOptions": {
    "baseUrl": "./e2e-tests",
    "module": "commonjs",
    "outDir": "./dist",
    "jsx": "react-jsxdev",
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": false,
    "declarationMap": false,
    "noImplicitAny": true,
