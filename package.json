{
  "name": "appformer-js",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "init": "npx lerna bootstrap",
    "build": "npx lerna run build --stream",
    "build:fast": "npx lerna run build:fast --stream",
    "build:prod": "npx lerna run build:prod --stream",
    "prebuild": "yarn clean",
    "build:webpack": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --hot --color --progress --info=true --config webpack.dev.js",
    "test": "jest",
    "lint": "tslint -c ./tslint.json --project .",
    "format": "prettier --check --write ./src/**/*.{tsx,ts,js}",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "yarn build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@patternfly/patternfly": "^2.27.1",
    "@patternfly/react-charts": "^4.7.9",
    "@patternfly/react-core": "^3.87.3",
    "@patternfly/react-icons": "^3.14.3",
    "@patternfly/react-styles": "^3.5.13",
    "@patternfly/react-table": "^2.17.5",
    "@types/react-scrollbar": "^0.4.10",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scrollbar": "^0.5.6"
  },
  "devDependencies": {
    "lerna": "3.4.0",
    "@fortawesome/fontawesome-free": "^5.10.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-solid-svg-icons": "^5.10.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "@types/webpack": "^4.39.1",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "imagemin": "^7.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node": "^12.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.18.2",
    "raw-loader": "^3.1.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "svg-url-loader": "^3.0.0",
    "ts-loader": "^6.0.4",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "tslint-react-hooks": "^2.2.1",
    "typescript": "^3.6.2",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.3",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  }
}
