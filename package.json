{
  "private": true,
  "name": "kie-tools-root",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "bootstrap": "pnpm bootstrap:scripts && pnpm bootstrap:root && pnpm bootstrap:packages",
    "bootstrap:packages": "kie-tools--bootstrap",
    "bootstrap:root": "pnpm install --strict-peer-dependencies=false -F .",
    "bootstrap:scripts": "pnpm install --strict-peer-dependencies=false -F '{scripts/*}...'",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "update-version-to": "kie-tools--update-version-to"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@kie-tools-build/bootstrap": "workspace:*",
    "@kie-tools-build/build-env": "workspace:*",
    "@kie-tools-build/run-script-if": "workspace:*",
    "@kie-tools-build/sparse-checkout": "workspace:*",
    "@kie-tools-build/update-version": "workspace:*",
    "@nice-move/prettier-plugin-package-json": "^0.6.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.2",
    "@types/jest": "^26.0.23",
    "@types/jest-when": "^2.7.4",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-router": "^5.1.14",
    "@types/react-router-dom": "^5.1.7",
    "@types/testing-library__jest-dom": "^5.9.1",
    "@types/testing-library__react": "^9.1.2",
    "@types/testing-library__react-hooks": "^3.4.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "copy-webpack-plugin": "^8.1.1",
    "copyfiles": "^2.4.1",
    "cpr": "^3.0.1",
    "css-loader": "^5.2.6",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-replace-webpack-plugin": "^2.6.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-raw-loader": "^1.0.1",
    "jest-webextension-mock": "^3.7.19",
