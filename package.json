{
  "name": "kie-tools",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "examples/*"
    ]
  },
  "scripts": {
    "install-dependencies": "pnpm install --strict-peer-dependencies=false",
    "link-packages-with-self": "node ./scripts/link_packages_with_self.js",
    "generate-packages-graph": "node ./scripts/generate_packages_graph.js ./repo",
    "list-packages-dependencies": "node ./scripts/list_packages_dependencies.js",
    "check-cli-tools": "node ./scripts/check_cli_tools.js",
    "bootstrap": "pnpm install-dependencies && pnpm link-packages-with-self && pnpm generate-packages-graph && pnpm check-cli-tools",
    "update-version-to": "node ./scripts/update_version.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "graphviz": "^0.0.9",
    "graph-data-structure": "^2.0.0",
    "husky": "^6.0.0",
    "js-yaml": "^4.1.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "@pnpm/find-workspace-packages": "^4.0.26",
    "@pnpm/logger": "^4.0.0",
    "copyfiles": "^2.4.1",
    "cpr": "^3.0.1",
    "@types/node": "^15.0.2",
    "typescript": "^4.4.2",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-router": "^5.1.14",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "@types/jest": "^26.0.23",
    "@types/jest-when": "^2.7.4",
    "ts-jest": "^26.5.6",
    "jest": "^26.6.3",
    "jest-raw-loader": "^1.0.1",
    "jest-webextension-mock": "^3.7.19",
    "jest-when": "^3.5.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.2",
    "@types/testing-library__jest-dom": "^5.9.1",
    "@types/testing-library__react": "^9.1.2",
    "@types/testing-library__react-hooks": "^3.4.0",
    "copy-webpack-plugin": "^8.1.1",
    "html-replace-webpack-plugin": "^2.6.0",
    "html-webpack-plugin": "^5.3.2",
    "css-loader": "^5.2.6",
    "file-loader": "^6.2.0",
    "null-loader": "^4.0.1",
    "raw-loader": "^4.0.2",
    "sass-loader": "^12.3.0",
    "sass": "^1.43.4",
    "source-map-loader": "^2.0.2",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.1.2",
    "url-loader": "^4.1.1",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^3.0.0"
  },
  "resolutions": {
    "react-dropzone": "^11.4.2",
    "@types/react": "^17.0.6"
  },
  "pnpm": {
    "packageExtensions": {
      "monaco-editor-webpack-plugin": {
        "peerDependencies": {
          "monaco-yaml": "*"
        }
      }
    }
  }
}
