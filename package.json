    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "source-map-loader": "^2.0.2",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.1.2",
