    "pnpm": "7.0.0"
  },
  "pnpm": {
    "packageExtensions": {
      "monaco-editor-webpack-plugin": {
        "peerDependencies": {
          "monaco-yaml": "*"
        }
      }
    }
  },
  "kieTools": {
    "requiredPreinstalledCliCommands": [
      "pnpm",
      "node",
      "npm"
    ]
  }
}