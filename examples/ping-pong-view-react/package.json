{
  "name": "@kie-tools-examples/ping-pong-view-react",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "Apache-2.0",
  "private": true,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kie-tools.git"
  },
  "scripts": {
    "copy:css": "copyfiles -u 1 \"src/**/*.{sass,scss,css}\" dist/",
    "build": "rimraf dist && pnpm copy:css && tsc",
    "build:dev": "pnpm run-script-if --bool \"$(build-env global.build.examples)\" --then \"pnpm build\"",
    "build:prod": "pnpm run-script-if --bool \"$(build-env global.build.examples)\" --then \"pnpm build\""
  },
  "dependencies": {
    "@kie-tools-core/envelope-bus": "workspace:*",
    "@kie-tools-core/envelope": "workspace:*",
    "@kie-tools-examples/ping-pong-view": "workspace:*",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@kie-tools/build-env": "workspace:*",
    "@kie-tools-core/run-script-if": "workspace:*"
  }
}
