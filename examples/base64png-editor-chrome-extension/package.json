{
  "name": "@kie-tools-examples/base64png-editor-chrome-extension",
  "version": "0.0.0",
  "description": "",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kie-tools.git"
  },
  "scripts": {
    "build": "rimraf dist && webpack",
    "build:dev": "pnpm run-script-if --bool \"$(build-env global.build.examples)\" --then \"pnpm build --env dev\"",
    "build:prod": "pnpm run-script-if --bool \"$(build-env global.build.examples)\" --then \"pnpm build\"",
    "serve-envelope": "webpack-dev-server"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ]
  },
  "dependencies": {
    "@kie-tools-core/chrome-extension": "workspace:*",
    "@kie-tools-core/editor": "workspace:*",
    "@kie-tools-core/envelope-bus": "workspace:*",
    "@kie-tools-core/patternfly-base": "workspace:*",
    "@kie-tools-examples/base64png-editor": "workspace:*"
  },
  "devDependencies": {
    "@kie-tools/build-env": "workspace:*",
    "@kie-tools-core/run-script-if": "workspace:*",
    "@kie-tools-core/webpack-base": "workspace:*",
    "zip-webpack-plugin": "^4.0.1",
    "@types/chrome": "^0.0.193"
  }
}
