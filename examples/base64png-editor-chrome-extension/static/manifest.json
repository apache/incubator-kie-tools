{
  "name": "Kogito Base64 PNG Editor",
  "version": "0.0.0",
  "manifest_version": 2,
  "description": "Kogito Base64 PNG Editor",
  "content_scripts": [
    {
      "run_at": "document_idle",
      "js": ["contentscript.js"],
      "matches": ["https://github.com/*"],
      "all_frames": true
    }
  ],
  "icons": { "128": "resources/icon_128.png" },
  "web_accessible_resources": ["envelope/*", "resources/*"],
  "permissions": ["https://*.github.com/*", "http://*.github.com/*"]
}
