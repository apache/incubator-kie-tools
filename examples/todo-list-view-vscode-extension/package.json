{
  "publisher": "kie-tools-examples",
  "name": "kie-tools-examples-todo-list-view-vscode-extension",
  "displayName": "Todo List View :: Kogito Examples ",
  "description": "",
  "main": "./dist/extension.js",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "vscode": "^1.66.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kie-tools.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "scripts": {
    "pack": "vsce package --yarn -o ./dist/kie_tools_examples_todo_list_view_$npm_package_version.vsix",
    "compile": "pnpm build --env dev",
    "watch": "pnpm build --env dev",
    "build": "rimraf dist && webpack",
    "build:dev": "pnpm run-script-if --bool \"$(build-env global.build.examples)\" --then \"pnpm build --env dev\"",
    "build:prod": "pnpm run-script-if --bool \"$(build-env global.build.examples)\" --then \"pnpm build\" \"pnpm pack\""
  },
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "command": "kie-tools-examples.todo-list-view.add-item"
        }
      ]
    },
    "commands": [
      {
        "command": "kie-tools-examples.todo-list-view",
        "title": "Open list",
        "category": "TODO"
      },
      {
        "command": "kie-tools-examples.todo-list-view.add-item",
        "title": "TODO: Add item(s)"
      },
      {
        "command": "kie-tools-examples.todo-list-view.mark-all-as-completed",
        "title": "Mark all as completed",
        "category": "TODO"
      }
    ]
  },
  "dependencies": {
    "@kie-tools-core/vscode-extension": "workspace:*",
    "@kie-tools-examples/todo-list-view": "workspace:*",
    "@kie-tools-core/envelope-bus": "workspace:*"
  },
  "devDependencies": {
    "@kie-tools/build-env": "workspace:*",
    "@kie-tools-core/run-script-if": "workspace:*",
    "@kie-tools-core/webpack-base": "workspace:*",
    "@kie-tools-core/patternfly-base": "workspace:*",
    "@types/vscode": "^1.66.0",
    "vsce": "^2.6.5"
  }
}
